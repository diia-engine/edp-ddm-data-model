<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.2.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext xsd/dbchangelog-ext.xsd">

<!-- placeholder for registry data model objects -->
	<property  name="dbname"  value="registry"/>

    <preConditions>
		<sqlCheck expectedResult="t">
			SELECT current_database() = '${dbname}'
		</sqlCheck>
    </preConditions>

	<changeSet author="registry owner" id="tag database v-0.1" context="all">
	  <tagDatabase tag="v-0.1"/>
	</changeSet>

    <include file="changeLogs/tablesSubjects.xml"/>
