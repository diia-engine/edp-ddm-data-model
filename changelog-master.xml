<!--    <include file="changeLogs/distibuteTables.xml" labels="citus"/>-->
    <include file="changeLogs/createSearchConditions(MDTUDDM-2381).xml" context="pub"/>
    <include file="changeLogs/createViewsForAnalytics.xml" context="sub"/>
<!--    <include file="changeLogs/populateDictionaries.xml" context="pub"/>-->
    <include file="changeLogs/createViewsForDataLoad.xml" context="pub"/>
    <include file="changeLogs/populateDictionariesByProcAL.xml" context="pub"/>
<!--        <include file="changeLogs/populateDictionariesByProcWithFixedUUID.xml" context="pub"/>-->
    <!--    <include file="changeLogs/populateDictionariesWithFixedUUID.xml" context="pub"/>-->
